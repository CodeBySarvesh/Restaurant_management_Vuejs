ate</router-link>\r\n                <button v-on:click=\"deleteResturant(item.id)\" class=\"btn\">Delete</button>\r\n            </td>\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n  </template>\r\n\r\n\r\n  <script>\r\n  import axios from 'axios'\r\nimport Header from './Header.vue'\r\nexport default{\r\n    name:'Home',\r\n    data()\r\n    {\r\n      return{\r\n        name:'',\r\n        resturants:[],\r\n      }\r\n\r\n    },\r\n    components:{\r\n      Header\r\n    },\r\n    methods: {\r\n        async deleteResturant(id){\r\n            let result = await axios.delete('http://localhost:3000/restaurant/'+id)\r\n            console.log(result)\r\n            if(result.status==200){\r\n                alert(result.data)\r\n                this.loadData();\r\n                this.$router.push({name:\"Home\"})\r\n            }\r\n        },\r\n        async loadData()\r\n        {\r\n          let user=localStorage.getItem('user-info');\r\n          if (!user)\r\n          {\r\n            this.$router.push({name:'SignUp'})\r\n          }\r\n          let result = await axios.get('http://localhost:3000/restaurant/')\r\n          console.warn(result)\r\n          this.resturants=result.data\r\n        }\r\n      }, \r\n    async mounted()\r\n  {\r\n    this.loadData();\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\ntd{\r\n   width: 160px;\r\n   height: 40px;\r\n}\r\n\r\n.button3 {background-color: #f44336;}\r\n</style>"],"mappings":";AA8BE,OAAOA,KAAI,MAAO,OAAM;AAC1B,OAAOC,MAAK,MAAO,cAAa;AAChC,eAAc;EACVC,IAAI,EAAC,MAAM;EACXC,IAAIA,CAAA,EACJ;IACE,OAAM;MACJD,IAAI,EAAC,EAAE;MACPE,UAAU,EAAC;IACb;EAEF,CAAC;EACDC,UAAU,EAAC;IACTJ;EACF,CAAC;EACDK,OAAO,EAAE;IACL,MAAMC,eAAeA,CAACC,EAAE,EAAC;MACrB,IAAIC,MAAK,GAAI,MAAMT,KAAK,CAACU,MAAM,CAAC,mCAAmC,GAACF,EAAE;MACtEG,OAAO,CAACC,GAAG,CAACH,MAAM;MAClB,IAAGA,MAAM,CAACI,MAAM,IAAE,GAAG,EAAC;QAClBC,KAAK,CAACL,MAAM,CAACN,IAAI;QACjB,IAAI,CAACY,QAAQ,CAAC,CAAC;QACf,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;UAACf,IAAI,EAAC;QAAM,CAAC;MACnC;IACJ,CAAC;