{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport Header from './Header.vue';\nexport default {\n  name: 'Add',\n  components: {\n    Header\n  },\n  data() {\n    return {\n      addname: '',\n      address: '',\n      contact: '',\n      name: '',\n      resturants: []\n    };\n  },\n  methods: {\n    async deleteResturant(id) {\n      let result = await axios.delete('http://localhost:3000/restaurant/' + id);\n      console.log(result);\n      if (result.status == 200) {\n        alert(\"Restaurant deleted successfully\");\n        this.loadData();\n        this.$router.push({\n          name: \"Add\"\n        });\n      }\n    },\n    async loadData() {\n      let user = localStorage.getItem('user-info');\n      if (!user) {\n        this.$router.push({\n          name: 'SignUp'\n        });\n      }\n      let result = await axios.get('http://localhost:3000/restaurant/');\n      console.warn(result);\n      this.resturants = result.data;\n    },\n    async addRestaurent() {\n      let result = await axios.post('http://localhost:3000/restaurant/', {\n        name: this.addname,\n        address: this.address,\n        contact: this.contact\n      });\n      if (result.status == 201) {\n        alert('Resturant Added Successfully');\n        this.loadData();\n        this.$router.push({\n          name: \"Add\"\n        });\n      }\n    }\n  },\n  mounted() {\n    this.loadData();\n  }\n};","map":{"version":3,"names":["axios","Header","name","components","data","addname","address","contact","resturants","methods","deleteResturant","id","result","delete","console","log","status","alert","loadData","$router","push","user","localStorage","getItem","get","warn","addRestaurent","post","mounted"],"sources":["D:\\Restarunt_Management_VueJs_Project\\resto-project\\src\\components\\Add.vue"],"sourcesContent":["<template>\r\n    <Header />    \r\n    <div class=\"headContainer\">\r\n        <div class=\"container-2\">\r\n            <h1> Add Restaurant</h1>\r\n            <div class=\"add\">\r\n                <input type=\"text\" name=\"addname\" placeholder=\"Enter Restaurent Name\" v-model=\"addname\"/>\r\n                <input type=\"text\" name=\"address\" placeholder=\"Enter Restaurent Address\" v-model=\"address\"/>\r\n                <input type=\"text\" name=\"contact\" placeholder=\"Enter Restaurent Contact\" v-model=\"contact\"/>\r\n                <button type=\"button\" v-on:click=\"addRestaurent\">Add Restaurant</button>\r\n            </div>\r\n            \r\n        </div>\r\n    \r\n        <div class=\"container-1\">\r\n    \r\n            <table border=\"1\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>ID</th>\r\n                        <th>Name</th>\r\n                        <th>Address</th>\r\n                        <th>Contact</th>\r\n                        <th>Actions</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"item in resturants\" :key=\"item.id\">\r\n                        <td>{{ item.id }}</td>\r\n                        <td>{{ item.name }}</td>\r\n                        <td>{{ item.address }}</td>\r\n                        <td>{{ item.contact }}</td>\r\n                        <td>\r\n                            <router-link :to=\"'/update/'+item.id \">Update</router-link>\r\n                            <button v-on:click=\"deleteResturant(item.id)\" class=\"btn\">Delete</button>\r\n                        </td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n       \r\n\r\n\r\n    </div>\r\n    \r\n    </template>\r\n  \r\n  \r\n<script>\r\nimport axios from 'axios'\r\nimport Header from './Header.vue'\r\nexport default{\r\n    name:'Add',\r\n    components:{\r\n    Header\r\n    },\r\n    data()\r\n    {\r\n        return{\r\n            addname:'',\r\n            address:'',\r\n            contact:'',\r\n            name:'',\r\n            resturants:[],\r\n        }\r\n    },\r\n    methods:      \r\n    {\r\n        async deleteResturant(id){\r\n            let result = await axios.delete('http://localhost:3000/restaurant/'+id)\r\n            console.log(result)\r\n            if(result.status==200){\r\n                alert(\"Restaurant deleted successfully\")\r\n                this.loadData();\r\n                this.$router.push({name:\"Add\"})\r\n            }\r\n        },\r\n        async loadData()\r\n        {\r\n          let user=localStorage.getItem('user-info');\r\n          if (!user)\r\n          {\r\n            this.$router.push({name:'SignUp'})\r\n          }\r\n          let result = await axios.get('http://localhost:3000/restaurant/')\r\n          console.warn(result)\r\n          this.resturants=result.data\r\n        },\r\n    async addRestaurent()\r\n    {\r\n        let result = await axios.post('http://localhost:3000/restaurant/',{\r\n            name:this.addname,\r\n            address:this.address,\r\n            contact:this.contact\r\n        })\r\n        if (result.status==201){\r\n            alert('Resturant Added Successfully')\r\n            this.loadData();\r\n            this.$router.push({name:\"Add\"})\r\n        }\r\n    }\r\n    },\r\n    mounted()\r\n{\r\n    this.loadData();\r\n}\r\n}\r\n</script>\r\n\r\n<style>\r\n.container-2{\r\n   float: right;\r\n   width:30%;\r\n    border-left: 1px solid;\r\n    \r\n}\r\n\r\ntd{\r\n    width: 160px;\r\n    height: 40px;\r\n }\r\n \r\n .btn {\r\n   background-color: #f44336;\r\n   border-radius: 12px;\r\n   margin-left: 2px;\r\n \r\n }\r\n\r\n \r\n</style>"],"mappings":";AAiDA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,MAAK,MAAO,cAAa;AAChC,eAAc;EACVC,IAAI,EAAC,KAAK;EACVC,UAAU,EAAC;IACXF;EACA,CAAC;EACDG,IAAIA,CAAA,EACJ;IACI,OAAM;MACFC,OAAO,EAAC,EAAE;MACVC,OAAO,EAAC,EAAE;MACVC,OAAO,EAAC,EAAE;MACVL,IAAI,EAAC,EAAE;MACPM,UAAU,EAAC;IACf;EACJ,CAAC;EACDC,OAAO,EACP;IAC